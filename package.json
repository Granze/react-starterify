{
  "name": "react-starterify",
  "version": "1.0.3",
  "description": "React JS application skeleton using Browserify and other awesome tools",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Granze/react-starterify"
  },
  "keywords": [
    "gulp",
    "react",
    "browserify",
    "seed",
    "boilerplate",
    "kickstarter",
    "ES6",
    "6to5",
    "babel",
    "mocha",
    "chai"
  ],
  "scripts": {
    "start": "npm run watch",
    "test": "mocha --reporter nyan --compilers js:babel/register --recursive",
    "clean": "rimraf dist",
    "lint": "eslint src/**/*.jsx",
    "build": "",
    "build-js": "browserify src/app.jsx -t babelify -d -p [minifyify --map dist/js/app.map.json --output dist/js/app.map.json] > dist/js/app.js",
    "build-css": "node-sass styles/main.scss | postcss --use autoprefixer | cssnano > dist/css/main.css",
    "watch": "",
    "watch-js": "watchify -t babelify src/app.jsx -o dist/js/app.js",
    "watch-css": ""
  },
  "author": "Maurizio Mangione",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Granze/react-starterify/issues"
  },
  "homepage": "https://github.com/Granze/react-starterify",
  "dependencies": {
    "react": "^0.13.3",
    "superagent": "^1.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^5.2.0",
    "babel": "^5.6.3",
    "babel-eslint": "^3.1.17",
    "babelify": "^6.1.2",
    "browser-sync": "^2.7.12",
    "browserify": "^10.2.4",
    "chai": "^3.0.0",
    "cssnano": "^1.4.3",
    "eslint": "^0.23.0",
    "eslint-plugin-react": "^2.5.2",
    "minifyify": "^7.0.1",
    "mocha": "^2.2.5",
    "node-sass": "^3.2.0",
    "postcss-cli": "^1.3.1",
    "react-tools": "^0.13.3",
    "rimraf": "^2.4.0",
    "watchify": "^3.2.2"
  }
}
